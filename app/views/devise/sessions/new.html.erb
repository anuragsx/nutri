<div class="regouter">
  <div class="container">
     <div class="row">
       <div class="col-xs-12 col-sm-12 col-md-6 col-md-offset-3 col-lg-6 col-lg-offset-3">
          <div class="regarea">
            <h2>Login </h2>
            <div class="regform">
              <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
                <%= devise_error_messages! %>    
                <%= f.label :email, class: "control-label", style: "float:left; font-size:20px;" %><br />
                <%= f.email_field :email, autofocus: true, placeholder: "Enter Name" %>
                <br><br>
                <%= f.label :password, class: "control-label", style: "float:left; font-size:20px;" %><br />                   
                <%= f.password_field :password, autofocus: true, placeholder: "Enter Password", autocomplete: "off" %>
                <%= f.submit "Log in", class: "subbtn" %>
                <p>Already have an account? </p>
                <%= render "devise/shared/links" %>
                <% end %>
            </div>
          </div>
         </div>
      </div>
   </div>
</div>
